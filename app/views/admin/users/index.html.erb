
<h1>全ユーザー一覧</h1>

<table class="table">

<tr>
  <th>ユーザー名</th>
  <th>メールアドレス</th>
</tr>
 <% @users.each do |user| %>
<tr>

  <td><%= link_to admin_user_path(user) do %>
  <%= user.name %>
  <% end %>
  </td>
  <td><%= user.email %></td>
  <td><%= link_to '削除', admin_user_path(user), method: :delete, data: { confirm: '本当に削除しますか？' } %> </td>
  <% end %>
</tr>

</table>